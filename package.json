{
  "name": "wykop",
  "version": "1.1.16",
  "description": "Wrapper for the new Wykop API v3 written in NodeJS",
  "homepage": "https://github.com/xxdeepfriedxx/wykop",
  "repository": "https://github.com/xxdeepfriedxx/wykop",
  "license": "SEE LICENSE IN LICENSE",
  "main": "wykop.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "xxdeepfriedxx",
  "dependencies": {
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "eslint": "^8.47.0"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "indent": [
        "error",
        "tab"
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
}
